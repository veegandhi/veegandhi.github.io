{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://veegandhi.github.io",
  "devDependencies": {
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.1.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "scripts": {
    "build-sass-watch": "node-sass-chokidar ./src/scss -o ./src/styles --watch",
    "build-sass": "node-sass-chokidar ./src/scss -o ./src/styles",
    "start-react": "react-scripts start",
    "start": "npm-run-all -p build-sass-watch start-react",
    "build": "npm-run-all -p build-sass -s build-app",
    "build-app": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  }
}
